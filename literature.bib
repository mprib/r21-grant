@ARTICLE{Hsiao2016-tr,
  title    = "Mechanisms used to increase peak propulsive force following
              12-weeks of gait training in individuals poststroke",
  author   = "Hsiao, Haoyuan and Knarr, Brian A and Pohlig, Ryan T and
              Higginson, Jill S and Binder-Macleod, Stuart A",
  abstract = "Current rehabilitation efforts for individuals poststroke focus
