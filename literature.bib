@ARTICLE{Cools2007-oo,
  title    = "Rehabilitation of scapular muscle balance: which exercises to
              prescribe?",
  author   = "Cools, Ann M and Dewitte, Vincent and Lanszweert, Frederick and
              Notebaert, Dries and Roets, Arne and Soetens, Barbara and Cagnie,
